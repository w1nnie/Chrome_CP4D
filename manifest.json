{
    "manifest_version": 2,
    "author": "",
    "homepage_url": "https://twitter.com/_winnie_ill",
    "name": "Colorpicker",
    "description": "color",
    "version": "0.0.1",
    "icons": {
        "16": "image/16.png",
        "48": "image/48.png",
        "128": "image/128.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/content.js"]
    }],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "image/48.png",
        "default_title": "kusa"
    },
    "permissions": [
        "tabs",
        "background",
        "storage",
        "activeTab"
    ],
    "web_accessible_resources": ["js/*"]
}
